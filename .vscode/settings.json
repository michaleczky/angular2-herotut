// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        // hide compiled js files under app directory
        "**/app/*.js": true,
        "**/app/*.map": true
    }
}